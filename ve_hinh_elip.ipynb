   "source": [
    "import turtle\n",
    "t = turtle.Turtle()\n",
    "ban_kinh = 100\n",
    "t.penup()\n",
    "t.goto(-ban_kinh,-ban_kinh/2)\n",
    "t.right(45)\n",
    "t.pendown()\n",
    "i = 0\n",
    "while i < 2:\n",
    "    t.circle(ban_kinh,90)\n",
    "    t.circle(ban_kinh/2,90)\n",
    "    i += 1\n",
    "turtle.done()"
   ]
